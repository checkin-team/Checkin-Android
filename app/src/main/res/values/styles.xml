<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="Base.AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primary_red</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="actionBarStyle">@style/AppTheme.AppBarOverlay</item>
        <item name="android:fontFamily">@font/opensans</item>
    </style>

    <style name="AppTheme" parent="Base.AppTheme" />

    <style name="MyCheckBox" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/brownish_grey</item>
        <item name="colorControlActivated">@color/primary_red</item>
    </style>

    <style name="appbar_main" parent="AppTheme.AppBarOverlay">
        <item name="android:homeAsUpIndicator">@drawable/ic_back_grey</item>
        <item name="elevation">@dimen/spacing_elevation_fix_extreme_tiny</item>
    </style>

    <style name="SessionTheme" parent="Theme.AppCompat.Light.NoActionBar">
