<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipeRevealLayout">
        <attr name="dragFromEdge">
            <flag name="left" value="1" />
            <flag name="right" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MultiSpinner">
        <attr name="entries" format="reference" />
        <attr name="values" format="reference" />
        <attr name="hintText" format="string" />
        <attr name="staticText" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="trimLength" format="integer" />
        <attr name="selectedMode">
            <enum name="line" value="0" />
            <enum name="length" value="1" />
        </attr>
        <attr name="trimLines" format="integer" />
    </declare-styleable>
    <declare-styleable name="SectionFragmentHolderView">
        <attr name="sectionText" format="string" />
    </declare-styleable>
</resources>
